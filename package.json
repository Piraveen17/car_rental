    "autoprefixer": "^10.4.23",
    "jest": "*",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-jest": "*",
    "tw-animate-css": "1.3.3",
    "typescript": "^5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx",
      "**/__tests__/unit/**/*.test.ts",
      "**/__tests__/integration/**/*.test.ts",
      "**/__tests__/acceptance/**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "jsx": "react-jsx",
            "moduleResolution": "node",
            "module": "commonjs",
            "esModuleInterop": true,
            "paths": {
              "@/*": [
                "./*"
              ]
            }
          }
        }
      ]
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "app/api/**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  }
}
